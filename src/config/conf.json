{
    "markdown_config": {
        "show-api-request-log": false,
        "use-common-mark": false,
        "parse-html": true,
        "all-links": false,
        "close-single-tag": false,
        "break-tag-linebreaks": true,
        "typographer": true,
        "link_attributes": [{
            "use-pattern": false,
            "pattern": "/^https?:\/\//",
            "attr": {
                "target": "_blank",
                "rel": "noopener",
                "className": "md-links"
            }
        }],
        "page-map": [{
            "requestedAs": "home-page-markdown",
            "located": "README.md"
        }],
        "error-messages": {
            "use-defaults": true,
            "custom-messages": {
                "invPath": "<type custom message for invPath error>",
                "fileNotExist": "<type custom message for fileNotExist error>",
                "other": "<type custom message for other error>",
                "default": "<type custom message for default error>"
            }
        }
    },
    "app_config": {
        "displayBodyText": false,
        "node_path": [
            "./",
            "./src",
            "./src/components",
            "./src/components/core",
            "./src/utils/built-in",
            "./src/styles"
        ],
        "showLayoutProps": "all",
        "styles": [
            "https://fonts.googleapis.com/css?family=Kanit|Lato&display=swap"
        ]
    },
    "layout_config": {
        "general": {
            "separate-layout-with-hr": true
        },
        "nav": {
            "useBrandOrTitle": "brand",
            "brand": "USING BRAND",
            "classes": {
                "main": {
                    "display": "flex"
                },
                "navbar-left": {
                    "textAlign": "left"
                },
                "navbar-right": {
                    "flex": 1,
                    "textAlign": "right",
                    "paddingRight": "20px"
                }
            },
            "links": [{
                    "href": "/",
                    "label": "Home",
                    "as": "home",
                    "title": "Home Page"
                },
                {
                    "href": "/page-1",
                    "label": "Page One",
                    "as": "page-one",
                    "title": "Page test",
                    "subtitle": true,
                    "subTitleText": "This is a test page"
                },
                {
                    "href": "/page-2",
                    "label": "Page Two",
                    "as": "page-Tow",
                    "title": "Page Two test",
                    "subtitle": true,
                    "subTitle": "This is a test page"
                },
                {
                    "href": "/not-found",
                    "label": "Not found",
                    "as": "contact",
                    "title": "Not found Test Page",
                    "subtitle": true,
                    "subTitle": "When the markdown file does not exist"
                },
                {
                    "href": "page-error",
                    "label": "Not implemented",
                    "as": "page-error",
                    "title": "THIS IS AN ERROR!",
                    "subtitle": true,
                    "subTitle": "When the page is not created in the `pages` folder"
                }
            ]
        },
        "header": {
            "classes": {
                "main": {
                    "display": "block"
                }
            }
        },
        "main": {
            "classes": {
                "main": {
                    "display": "block"
                }
            }
        },
        "footer": {
            "classes": {
                "main": {
                    "display": "block"
                }
            }
        }
    },
    "dev-config": {
        "display_App_component_mount": true,
        "display_App_component_update": true
    }
}